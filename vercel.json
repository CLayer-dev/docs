{
    "builds": [
        {
            "src": "api/**/*.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "env": {
        "GROK_API_KEY": "@grok_api_key",
        "Circle_Layer_Docs_API_KEY": "@circle_layer_docs_api_key"
    }
}