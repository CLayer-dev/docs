{
    "version": 2,
    "name": "circle-layer-docs",
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "devCommand": "npm start",
    "installCommand": "npm install",
    "framework": null,
    "public": false,
    "env": {
        "NODE_ENV": "production"
    },
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ]
}